# Agent ID - Must be unique for each agent
# Use 'agent-1' on first computer, 'agent-2' on second computer
AGENT_ID=agent-1

# Sync interval in milliseconds (default: 30000 = 30 seconds)
SYNC_INTERVAL=30000

# Heartbeat interval in milliseconds (default: 10000 = 10 seconds)
HEARTBEAT_INTERVAL=10000
